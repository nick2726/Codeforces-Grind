https://codeforces.com/problemset/problem/122/A


Solution:
#include <bits/stdc++.h>

using namespace std;

int main() {
        // your code goes here
        int t;
        cin >> t;
        while (t--) {
            int n;
            cin >> n;
            int maxi = 0;
            for (int a = 1; a <= n/2; a++) {
                int b = a;
                int curr_gcd=__gcd(a,b);
                maxi= max(maxi,curr_gcd);
                }
                cout << maxi << "\n";
            }
            return 0;
        }
